    "@types/jest": "^23.3.1",
    "@types/pegjs": "^0.10.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "jest": "^23.4.2",
    "npm-run-all": "^4.1.3",
    "pegjs": "^0.10.0",
    "pegjs-jest": "^0.0.2",
    "pegjs-loader": "^0.5.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.5",
    "rollup-plugin-pegjs": "^2.1.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.16.1",
